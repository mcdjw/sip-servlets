        <wadl:resource path="/dynamic-event-service">
          <wadl:doc title="action dynamic-event-service">manage dynamic web service event listening endpoints</wadl:doc>
          <wadl:resource path="/register">
            <wadl:doc title="action dynamic-event-service register">add a new web service event listener</wadl:doc>
            <wadl:method name="GET" id="action-dynamic-event-service-register">
              <wadl:request>
                <wadl:param name="endpoint">
                  <wadl:doc>the URL to the SOAP endpoint that will process the events</wadl:doc>
                </wadl:param>
                <wadl:param name="channels">
                  <wadl:doc>the pipe delimited list of channels to register for</wadl:doc>
                </wadl:param>
                <wadl:param name="xmlFormat" default="simplified">
                  <wadl:doc>The XML format used with this server</wadl:doc>
                  <wadl:option value="legacy">
                    <wadl:doc>use the legacy format</wadl:doc>
                  </wadl:option>
                  <wadl:option value="simplified">
                    <wadl:doc>use the simplified format</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="timeToLive" default="untilRestart">
                  <wadl:doc>the time to live for this registration</wadl:doc>
                  <wadl:option value="untilRestart">
                    <wadl:doc>keep the registration alive until restart</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="connectTimeout" default="1000">
                  <wadl:doc>the length of time to use to try and connect to this endpoint in milliseconds</wadl:doc>
                </wadl:param>
                <wadl:param name="readTimeout" default="1000">
                  <wadl:doc>the length of time to use to try and read from this endpoint in milliseconds</wadl:doc>
                </wadl:param>
                <wadl:param name="characterSet" default="utf-8">
                  <wadl:doc>the character set to use when forming requests to this endpoint</wadl:doc>
                  <wadl:option value="iso-8859-1">
                    <wadl:doc>The ISO-8859-1 character encoding.</wadl:doc>
                  </wadl:option>
                  <wadl:option value="utf-8">
                    <wadl:doc>The UTF-8 character encoding.</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="requestStyle" default="soap">
                  <wadl:doc>the format to use when sending events to this listener</wadl:doc>
                  <wadl:option value="json">
                    <wadl:doc>a REST based JavaScript Object Notation event listener</wadl:doc>
                  </wadl:option>
                  <wadl:option value="soap">
                    <wadl:doc>a SOAP based XML event listener</wadl:doc>
                  </wadl:option>
                  <wadl:option value="xml">
                    <wadl:doc>a REST based XML event listener</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="includeChannelsInEvents" default="enabled">
                  <wadl:doc>flag indicating whether event notifications should include the channel or not</wadl:doc>
                  <wadl:option value="disabled">
                    <wadl:doc>Resource is inactive</wadl:doc>
                  </wadl:option>
                  <wadl:option value="enabled">
                    <wadl:doc>Resource is active</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="backupEndpoint">
                  <wadl:doc>the URL to the SOAP endpoint that will process the events in the event that the primary endpoint fails</wadl:doc>
                </wadl:param>
                <wadl:param name="eventQueueDepth" default="1000">
                  <wadl:doc>The maximum number of event messages queued to be sent before event messages start being dropped</wadl:doc>
                </wadl:param>
                <wadl:param name="idleSenderThreads" default="1">
                  <wadl:doc>The number of event sending threads that are allowed to be idle</wadl:doc>
                </wadl:param>
                <wadl:param name="maxSenderThreads" default="5">
                  <wadl:doc>The maximum number of event sending threads that are allowed</wadl:doc>
                </wadl:param>
                <wadl:param name="output" style="query" default="xml">
                  <wadl:option value="xml" mediaType="text/xml">
                    <wadl:doc>output the response in XML</wadl:doc>
                  </wadl:option>
                  <wadl:option value="json" mediaType="application/json">
                    <wadl:doc>output the response in JSON format</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="_format" style="query" default="legacy">
                  <wadl:option value="legacy">
                    <wadl:doc>use the legacy format</wadl:doc>
                  </wadl:option>
                  <wadl:option value="simplified">
                    <wadl:doc>use the simplified format</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="callback" style="query" required="false">
                  <wadl:doc>specify the callback function for JSON-P style cross site callbacks</wadl:doc>
                </wadl:param>
                <wadl:param name="cxcip" style="query">
                  <wadl:doc>for a cluster, execute the action on a specific ASC</wadl:doc>
                </wadl:param>
                <wadl:param name="_timeout" style="query" default="30000">
                  <wadl:doc>the timeout to use while invoking the action</wadl:doc>
                </wadl:param>
              </wadl:request>
              <wadl:response status="200">
                <wadl:representation mediaType="text/xml" element="ExtActionResponse" />
                <wadl:representation mediaType="application/json" />
              </wadl:response>
            </wadl:method>
          </wadl:resource>
          <wadl:resource path="/keepalive">
            <wadl:doc title="action dynamic-event-service keepalive">update an existing web service event listener registration</wadl:doc>
            <wadl:method name="GET" id="action-dynamic-event-service-keepalive">
