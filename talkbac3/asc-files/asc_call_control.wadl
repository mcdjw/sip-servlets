<wadl:application xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:wadl="http://wadl.dev.java.net/2009/02">
  <wadl:doc>call-control</wadl:doc>
  <wadl:grammars>
    <wadl:include href="asc_rest.xsd" />
  </wadl:grammars>
  <wadl:resources base="http://cxcws:8080">
    <wadl:resource path="/cms">
      <!--==============================================================================-->
      <!--     Access                                                                   -->
      <!--==============================================================================-->
      <wadl:method name="GET" id="access-login">
        <wadl:doc title="access-login">administration methods for logging in</wadl:doc>
        <wadl:request>
          <wadl:param name="login" style="query" fixed="true" default="true" required="true" />
          <wadl:param name="username" style="query" />
          <wadl:param name="password" style="query" />
        </wadl:request>
        <wadl:response status="200">
          <wadl:representation mediaType="text/xml" element="ExtActionResponse" />
          <wadl:representation mediaType="application/json" />
        </wadl:response>
      </wadl:method>
      <wadl:method name="GET" id="access-logout">
        <wadl:doc title="access-logout">administration methods for logging out</wadl:doc>
        <wadl:request>
          <wadl:param name="logout" style="query" fixed="true" default="true" required="true" />
        </wadl:request>
        <wadl:response status="200">
          <wadl:representation mediaType="text/xml" element="ExtActionResponse" />
          <wadl:representation mediaType="application/json" />
        </wadl:response>
      </wadl:method>
      <wadl:method name="GET" id="access-keepalive">
        <wadl:doc title="access-keepalive">administration methods for keeping a session alive</wadl:doc>
        <wadl:request>
          <wadl:param name="keepalive" style="query" fixed="true" default="true" required="true" />
        </wadl:request>
        <wadl:response status="200">
          <wadl:representation mediaType="text/xml" element="ExtActionResponse" />
          <wadl:representation mediaType="application/json" />
        </wadl:response>
      </wadl:method>
      <!--==============================================================================-->
      <!--     Actions                                                                  -->
      <!--==============================================================================-->
      <wadl:resource path="/action">
        <wadl:resource path="/call-control">
          <wadl:doc title="action call-control">Create and control an established call</wadl:doc>
          <wadl:resource path="/call">
            <wadl:doc title="action call-control call">Place a voice call</wadl:doc>
            <wadl:method name="GET" id="action-call-control-call">
              <wadl:request>
                <wadl:param name="to">
                  <wadl:doc>The endpoint that is terminating the call</wadl:doc>
                </wadl:param>
                <wadl:param name="from">
                  <wadl:doc>The endpoint that is originating the call</wadl:doc>
                </wadl:param>
                <wadl:param name="requestId">
                  <wadl:doc>this id will be returned in all events</wadl:doc>
                </wadl:param>
                <wadl:param name="originatorFirst" default="enabled">
                  <wadl:doc>establish originator call-leg first</wadl:doc>
                  <wadl:option value="disabled">
                    <wadl:doc>Resource is inactive</wadl:doc>
                  </wadl:option>
                  <wadl:option value="enabled">
                    <wadl:doc>Resource is active</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="async" default="disabled">
                  <wadl:doc>action completes immediately (doesn't wait for connect)</wadl:doc>
                  <wadl:option value="disabled">
                    <wadl:doc>Resource is inactive</wadl:doc>
                  </wadl:option>
                  <wadl:option value="enabled">
                    <wadl:doc>Resource is active</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="transport">
                  <wadl:doc>The transport type to use</wadl:doc>
                  <wadl:option value="any">
                    <wadl:doc>All protocol types</wadl:doc>
                  </wadl:option>
                  <wadl:option value="TCP">
                    <wadl:doc>Transmission Control Protocol</wadl:doc>
                  </wadl:option>
                  <wadl:option value="TLS">
                    <wadl:doc>Transport Layer Security</wadl:doc>
                  </wadl:option>
                  <wadl:option value="UDP">
                    <wadl:doc>User Datagram Protocol</wadl:doc>
                  </wadl:option>
                  <wadl:option value="WS">
                    <wadl:doc>WebSocket</wadl:doc>
                  </wadl:option>
                  <wadl:option value="WSS">
                    <wadl:doc>Secure WebSocket</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="config">
                  <wadl:doc>Reference to a session configuration to use to setup the call</wadl:doc>
                </wadl:param>
                <wadl:param name="sessionID">
                  <wadl:doc>Optional session ID for a session</wadl:doc>
                </wadl:param>
                <wadl:param name="addlSessConfig"></wadl:param>
                <wadl:param name="serverConfig" style="query">
                  <wadl:doc>Reference to a 3PCC server that will process this call attempt</wadl:doc>
                </wadl:param>
                <wadl:param name="output" style="query" default="xml">
                  <wadl:option value="xml" mediaType="text/xml">
                    <wadl:doc>output the response in XML</wadl:doc>
                  </wadl:option>
                  <wadl:option value="json" mediaType="application/json">
                    <wadl:doc>output the response in JSON format</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="_format" style="query" default="legacy">
                  <wadl:option value="legacy">
                    <wadl:doc>use the legacy format</wadl:doc>
                  </wadl:option>
                  <wadl:option value="simplified">
                    <wadl:doc>use the simplified format</wadl:doc>
                  </wadl:option>
                </wadl:param>
                <wadl:param name="callback" style="query" required="false">
                  <wadl:doc>specify the callback function for JSON-P style cross site callbacks</wadl:doc>
                </wadl:param>
                <wadl:param name="cxcip" style="query">
                  <wadl:doc>for a cluster, execute the action on a specific ASC</wadl:doc>
                </wadl:param>
                <wadl:param name="_timeout" style="query" default="30000">
                  <wadl:doc>the timeout to use while invoking the action</wadl:doc>
                </wadl:param>
              </wadl:request>
              <wadl:response status="200">
                <wadl:representation mediaType="text/xml" element="ExtActionResponse" />
                <wadl:representation mediaType="application/json" />
              </wadl:response>
            </wadl:method>
          </wadl:resource>
